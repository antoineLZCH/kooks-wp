{% extends "base.twig" %}
{% block content %}
  {% include 'components/headers/post-header.twig' %}
  <div class="container">
    {% for post in posts %}
      <div class="lg:w-8/12 mx-auto">
        {{ post.content }}
      </div>
    {% endfor %}
  </div>
  {% include 'components/articles/footer-post.twig' with {next_article: next_article, previous_article: previous_article, articles_redirection: articles_redirection} %}
{% endblock %}
